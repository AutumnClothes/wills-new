/* pages/clienta/usertag/usertag.wxss */
.container{
  position: absolute;
  box-sizing: border-box;
  overflow:hidden;
  top: 0;
  left:0;
  bottom: 0;
  right: 0;
}
.bottom-box{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  border: 2px solid #f0f1f6;
  box-sizing: border-box;
}
/*标签页*/
.usertag-ul{
  box-sizing: border-box;
  position: relative;
  border:1px solid #f0f1f6;
}
.usertag-li-f{
  box-sizing: border-box;
  font-size: 32rpx;
  font-weight: 600;
  width: 100%;
  height:88rpx;
  line-height: 88rpx;
  padding-left:60rpx;
}
.usertag-li-icon{
  width: 8rpx;
  height:28rpx;
  position: absolute;
  top:32rpx;
  left:30rpx;
}
.usertag-li-l{
  display: inline-block;
  width: 100%;
  padding-left:10rpx;
  clear: none;
}
.usertag-li-v {
  font-size: 28rpx;
  float: left;
  padding: 0 24rpx;
  margin:8rpx 0 0 20rpx;
  border-radius: 26rpx;
  background-color: #f5f5f5;
  height: 52pxrpx;
  line-height: 52rpx;
  text-align: center;
  color: #515151;
  clear: none;
}
.usertag-li-v-true{
  font-size: 28rpx;
  float: left;
  padding: 0 24rpx;
  margin:8rpx 0 0 20rpx;
  border-radius: 26rpx;
  background-color: #ff8a01;
  height: 52pxrpx;
  line-height: 52rpx;
  text-align: center;
  color: #fefefe;
  clear: none;
}
.usertag-li-l text{
  color: #999
}
.bottom-box-one{
  position: absolute;
  bottom: 0;
  z-index: 999;
  background-color: #fff;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 4%;
  background-position: 37% 57%;
}
/*标签管理*/
.icon-left{
  float: left;
  height:60rpx;
  width: 60rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 50%;
  background-position: 50% 100%;
}
.icon-right{
  float: right;
  height:60rpx;
  width: 60rpx;
}
.icon-right-one{
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 50%;
  background-position: 50% 100%;
}
.icon-right-del{
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 50%;
  background-position: 50% 100%;
}
.icon-right-add{
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 50%;
  background-position: 50% 100%;
}
.bottom-box-two view{
  float: left;
  width: 50%;
  clear: none;
  box-sizing: border-box;
}
.bottom-box-two view:first-child{
  border-right:1rpx solid #f0f1f6;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 8%;
  background-position: 20% 52%;
}
.bottom-box-two view:last-child{
  border-left:1rpx solid #f0f1f6;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 8%;
  background-position: 20% 52%;
}
.bottom-box-two text{
  color: #ff8a01
}
/*点击修改后弹出的input*/
.alter-input{
  float: left;
  margin-top:20rpx;
  width:320rpx;
  border:1rpx solid #f0f1f6
}
/*返回按钮.css*/
.back-box{
  position: fixed;
  float: left;
  width:37rpx;
  height: 37rpx;
  margin-left:20rpx;
  background-repeat: no-repeat;
  background-size: 40%;
  background-position: 20% 100%;
  background-image: url(data:image/png;base64,)
}