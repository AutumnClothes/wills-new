/* pages/apipageone/apipageone.wxss */
/*顶部搜索框样式*/
page{background-color: #fff;}
.search-box{
  background-color: #fff;
  padding:8rpx 16rpx;
  padding-right:0; 
}
.search icon{
  position: absolute;
  padding:21rpx;
  padding-top:22rpx; 
  z-index: 999;
}
.search input{
  background-color: #efeff4;
  font-size: 26rpx;
  border-radius: 10rpx;
  vertical-align: middle;
  width: 634rpx;
  height: 68rpx;
  padding-left:57rpx; 
  box-sizing: border-box;
}
.label-box{
  position: absolute;
  right:0;
  top:10rpx;
  text-align: center;
  box-sizing: border-box;
  width: 100rpx;
  font-size:20rpx; 
  height: 68rpx;
  line-height: 115rpx;
  
    background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center 0;
  background-size: 38%;
}
.label-box-cancel{
  position: absolute;
  right:0;
  top:10rpx;
  text-align: center;
  width: 100rpx;
  font-size:30rpx; 
  color: #ffa134;
  height: 68rpx;
  line-height: 68rpx;
  background: #fff;
  z-index: 999;
}
/*tab分页*/
.tab-box{
  height:80rpx;
  padding:0 120rpx;
}
.i-tab{
  
}
/*用户index*/

.i-index-demo-item{
  height: 110rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.i-index-demo-item:last-child{
  border: none;
}
.i-index-demo-item view{
  vertical-align: middle
}
.portrait-box{
  margin-left: 0;
}
/*搜索弹出层样式*/
.soulist-box{
  position: absolute;
  width: 100%;
  height:100%;
  z-index: 999;
  border-top: 1px solid #efeff4;
  box-shadow:0 10rpx 10rpx 0rpx #efeff4 inset;
  background:#fff;
  padding-left:20rpx 
}
.i-index-demo-item rich-text{
  position: absolute;
  transform: translate(0%,50%)
}