.content-box{
  background-color: #f0f1f6;
  position: absolute;
  bottom: 0;
}
/*顶部tap样式*/
.tap-box{
  box-sizing: border-box;
  padding:0rpx 125rpx ;
  background: #fff;
  height: 86rpx;
  width: 100%;
  font-size: 30rpx;
}
.group-box{
  position: fixed;
  box-sizing: border-box;
  left:20rpx ;
  width:170rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: right;
  padding-right:30rpx; 
  border: 1rpx solid #eaeaea;
  border-radius: 37rpx;
  font-size: 32rpx;
  color: #000;
  background-repeat: no-repeat;
  background-size: 22%;
  background-position: 20% 50%;
  background-image: url(data:image/png;base64,)
}
/*聊天.css*/
.notice{
  box-sizing: border-box;
  height: 136rpx;
  line-height: 136rpx;
  border-radius: 10rpx;
  margin: 21rpx 25rpx;
  background-color: #fff;
  font-size: 32rpx;
  color: #ff8a01;
  padding-left: 150rpx;
}
.bell-box{
  width: 101rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: rgb(255, 240, 201);
  position: absolute;
  top: 126rpx;
  left:46rpx;
  background-repeat: no-repeat;
  background-size: 54%;
  background-position: center;
  background-image: url(data:image/png;base64,)
}
scroll-view {
  height:1000rpx;
}
.linkman-box{
  box-sizing: border-box;
  border-bottom: 1rpx solid #e5e5e5;
  background-color: #fff;
  height:136rpx;
  padding: 17rpx;
}
.linkman-portrait{
  display: inline-block;
  height: 101rpx;
  width: 100rpx;
  border-radius: 50%;
  background-color: #e5e5e5;
}
.linkman-text{
  display: inline-block;
  
  padding-left: 21rpx;
  transform: translateY(-10rpx)
}
.linkman-text view:nth-child(1){
  color: #000;
  font-size: 30rpx;
}
.linkman-text view:nth-child(2){
  color: #aaa;
  font-size: 27rpx;
}
.linkman-text view{
  height:45rpx;
  line-height: 54rpx;
  font-size: 25rpx;
}
.linkman-time{
  color:#dfdfdf;
  float: right;
  font-size: 24rpx;
  text-align: right;
  height:45rpx;
  line-height: 54rpx;
}
/*动态.css*/
.label-box{
  box-sizing: border-box;
  /* border: 1rpx solid red; */
  height: 88rpx;
  padding:16rpx;
  width: 100%;
  margin-top: 1rpx;
  background-color: #fff;
}
.label-itm{
  float: left;
  box-sizing: border-box;
  height:56rpx;
  line-height: 56rpx;
  text-align: center;
  width: 160rpx;
  border-radius: 28rpx;
  margin:0 10rpx;
  font-size: 28rpx;
  background-color: #f5f5f5
}
.label-install{
  float: right;
  height:34rpx;
  width:34rpx;
  margin:12rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  background-position: center;
  background-image: url(data:image/png;base64,)
}
.dynamic{
  position: relative;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 14rpx;
  height: 136rpx;
  margin: 20rpx 25rpx 0 25rpx;
  padding: 19rpx;
}
.dynamic-portrait{
  display: inline-block;
  box-sizing: border-box;
  border-radius: 50%;
  border: 1rpx solid #aaa;
  height:100rpx;
  width: 100rpx;
}
.dynamic-portrait-icon{
  position: absolute;
  top:15rpx;
  left:86rpx;
  height: 36rpx;
  line-height: 36rpx;
  color:#fff;
  width: 36rpx;
  font-size: 24rpx;
  text-align: center;
  border-radius: 50%;
  background-color: #ff5f19
}
.dynamic-text{
  position: absolute;
  top:50%;
  left:150rpx;
  transform: translateY(-50%);
  width: 480rpx;
  float: right;
  margin-right: 70rpx;
}
.dynamic-text text{
  float: left;
}
.dynamic-text-last{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 480rpx;
}
.dynamic-time{
  position: absolute;
  top:10rpx;
  right: 10rpx;
  color:#dfdfdf;
  float: right;
  font-size: 24rpx;
  text-align: right;
  height:45rpx;
  line-height: 54rpx;
}