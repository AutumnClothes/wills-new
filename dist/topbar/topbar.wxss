/* dist/topbar/topbar.wxss */
.top-box{
  position: fixed;
  top:0;
  font-size: 37rpx;
  font-family: "PingFangSC";
  width:100%;
  text-align: center;
  background: #fff;
  color:#38383c;
}
/*返回按钮.css*/
.back-box{
  position: fixed;
  float: left;
  width:37rpx;
  height: 37rpx;
  margin-left:20rpx;
  background-repeat: no-repeat;
  background-size: 40%;
  background-position: 20% 100%;
  background-image: url(data:image/png;base64,)
}